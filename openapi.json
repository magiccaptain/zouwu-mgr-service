    "/fund-account": {
      "get": {
        "operationId": "listFundAccounts",
        "summary": "",
        "description": "list fund accounts",
        "parameters": [
          {
            "name": "brokerKey",
            "required": false,
            "in": "query",
            "description": "券商key, eg: xtp, guojun",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "companyKey",
            "required": false,
            "in": "query",
            "description": "公司key, eg: zouwu, zhisui",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "productKey",
            "required": false,
            "in": "query",
            "description": "产品key, eg: zy1",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "active",
            "required": false,
            "in": "query",
            "description": "是否启用, 默认为 true",
            "schema": {
              "type": "boolean"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "list fund accounts",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/FundAccountEntity"
                  }
                }
              }
            }
          }
        }
      }
    },
