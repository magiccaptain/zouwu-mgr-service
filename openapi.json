        ],
        "responses": {
          "200": {
            "description": "进程检查结果"
          },
          "201": {
            "description": ""
          }
        },
        "tags": [
          "process-monitor"
        ]
      }
    },
    "/process-monitor/{id}/@kill": {
      "post": {
        "operationId": "killProcess",
        "summary": "杀死进程",
